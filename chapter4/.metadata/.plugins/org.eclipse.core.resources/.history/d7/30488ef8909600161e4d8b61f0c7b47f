package chapter4_java;

class Node{
	int data;
	Node left = null;
	Node right = null;
	
	public Node(int d){
		data = d;
	}
	
	public int getData(){
		return data;
	}
	
	public Node getLeft(){
		return left;
	}
	
	public Node getRight(){
		return right;
	}
	
	public void setLeft(Node n){
		left = n;
	}
	
	public void setRight(Node n){
		right = n;
	}
}

public class BinarySearchTree {
	Node root;
	public BinarySearchTree(){
		root = null;
	}
	
	public void insertIntoTree(Node temp,int data){
		if(temp == null){
			return;
		}
		if(data < temp.data){
			insertIntoTree(temp.getLeft(),data);
			Node n = new Node(data);
			temp.setLeft(n);
		}else if(data > temp.data){
			insertIntoTree(temp.getRight(),data);
			Node n = new Node(data);
			temp.setRight(n);
		}
	}
	
	public void display(Node temp){
		Node temp = root;
		while(temp != null){
			System.out.println(root.data)
		}
	}
	
	public void insert(int data){
		insertIntoTree(root,data);
	}
}
